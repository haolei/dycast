{
  "name": "@dycast/core",
  "version": "1.0.3",
  "description": "Core library for DyCast - Douyin live streaming message handling",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./server": {
      "types": "./dist/server-only.d.ts",
      "import": "./dist/server-only.mjs",
      "require": "./dist/server-only.js"
    }
  },
  "files": [
    "dist",
    "src",
    "public",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "build:server": "tsup src/server-only.ts --format cjs,esm --dts --platform node --target node16 --external ws --no-clean",
    "build:all": "npm run build && npm run build:server",
    "build:watch": "tsup src/index.ts --format cjs,esm --dts --watch",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run build:all"
  },
  "keywords": [
    "douyin",
    "live-streaming",
    "websocket",
    "dycast",
    "barrage",
    "chat"
  ],
  "author": "Your Name",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/dycast-core.git"
  },
  "dependencies": {
    "pako": "^2.1.0",
    "ws": "^8.0.0"
  },
  "peerDependencies": {
    "ws": "^8.0.0"
  },
  "peerDependenciesMeta": {
    "ws": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/pako": "^2.0.3",
    "@types/ws": "^8.0.0",
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "~5.8.0"
  },
  "engines": {
    "node": ">=16"
  }
}