{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "jsx": "preserve",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "baseUrl": ".",
    "paths": {
      "@dycast/core": ["../dycast-core/src/index.ts"],
      "@dycast/core/*": ["../dycast-core/src/*"]
    },
    "lib": ["DOM", "ESNext"],
    "types": ["vite/client"]
  },
  "include": ["src/**/*", "index.html"],
  "references": []
}
